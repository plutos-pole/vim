syntax on
set number
set relativenumber
set cursorline
set wrap
set textwidth=120
set shiftwidth=4
set tabstop=4
set expandtab
set nobackup
set incsearch
set showcmd
set showmode
set showmatch
set hlsearch
set wildmenu
set wildmode=list:longest
set noautoindent
set smartindent
set noswapfile
colorscheme quiet 



inoremap <c-b> <Esc>:Lex<cr>:vertical resize 30<cr>
nnoremap <c-b> <Esc>:Lex<cr>:vertical resize 30<cr>
